# Example .experia.env file for local E2E testing.
# DO NOT commit your real credentials. Copy this file to .experia.env and fill
# in the real values on your local machine.

EXPERIA_E2E=1
EXPERIA_IP=192.168.2.254
EXPERIA_USER=admin
EXPERIA_PASS=yourpassword_here
# Optional: expected connection state to assert in the e2e test
EXPERIA_EXPECT_CONNECTION_STATE=Connected
