# Example .experia.env for e2e testing (do NOT commit real secrets)
# Copy to .experia.env and populate real credentials for local testing
EXPERIA_E2E=1
EXPERIA_V10_ROUTER_IP=192.168.2.254
EXPERIA_V10_ROUTER_USERNAME=admin
EXPERIA_V10_ROUTER_PASSWORD=yourpassword_here
EXPERIA_V10_LISTEN_ADDR=:8081
EXPERIA_V10_TIMEOUT=5s
EXPERIA_EXPECT_CONNECTION_STATE=Connected
